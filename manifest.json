{
  "author": "sorae_42",
  "developer": {
    "name": "sorae_42",
    "url": "https://skyventuree.github.io/"
  },
  "applications": {
    "gecko": {
      "strict_min_version": "57.0"
    }
  },
  "background": {
      "scripts": ["background.js"]
  },
  "icons": {
    "32": "icons/find.svg"
  },
  "browser_action": {
    "browser_style": true,
    "default_title": "Show all tabs",
    "default_popup": "tabs.html",
    "default_icon": {
        "32": "icons/burger.svg"
    }
  },
  "description": "Find and switch to your tabs quickly.",
  "homepage_url": "https://github.com/skyventuree/tab-finder",
  "manifest_version": 2,
  "name": "Tab Finder",
  "permissions": [
    "tabs"
  ],
  "commands": {
    "_execute_browser_action": {
        "suggested_key": {
            "default": "Ctrl+Space",
            "mac": "MacCtrl+Space"
        }
    }
  },
  "version": "0.0.5"
}
